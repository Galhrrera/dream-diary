{
  "name": "dream-diary",
  "version": "1.0.0",
  "description": "Actividad evaluativa para el módulo 2 del curso de aplicaciones móviles 2023-20: Una aplicación híbrida",
  "source": "src/js/index.js",
  "main": "dist/index.js",
  "scripts": {
    "clean:dev": "rimraf ./dist",
    "clean:prod": "rimraf ./public",
    "preinstall": "npm install parcel-bundler",
    "dev": "npm run clean:dev && parcel ./src/*.html",
    "prod": "npm run clean:prod && parcel build ./src/*.html --dist-dir ./public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "rimraf": "^5.0.1",
    "sass": "^1.66.1"
  },
  "dependencies": {
    "parcel-bundler": "^1.12.5"
  }
}
